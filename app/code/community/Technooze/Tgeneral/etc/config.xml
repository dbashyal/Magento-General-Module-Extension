<config>
    <modules>
        <Technooze_Tgeneral>
            <version>0.1.0</version>
        </Technooze_Tgeneral>
    </modules>
    <global>
        <blocks>
            <tgeneral>
                <class>Technooze_Tgeneral_Blocks</class>
            </tgeneral>
            <page>
                <rewrite>
                    <!-- rewrite Page\Block\Html\Head.php -->
                    <html_head>Technooze_Tgeneral_Block_Html_Head</html_head>
                </rewrite>
            </page>
            <reports>
                <rewrite>
                    <!-- rewrite Reports\Block\Product\Viewed.php -->
